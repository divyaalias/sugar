wb = xlsx_package.workbook
bold = wb.styles.add_style(b: true)

if params[:from] && params[:to]
  wb.add_worksheet(name: "Month To Date") do |sheet|
    sheet.add_row [
       "Blood Level in mg/dl",
       "Date"
      ], style: bold
    @month_to_date.each do |button|
      sheet.add_row [
        button.blood_level, 
        button.created_at
      ]
    end
  end
end




